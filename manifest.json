{
  "name": "GGBGG",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Easily find games on BGG from MM and CSI listings.",
  "homepage_url": "https://github.com/maclennann/GGBGG",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "http://www.miniaturemarket.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://www.miniaturemarket.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
